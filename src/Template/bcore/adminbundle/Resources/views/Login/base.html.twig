<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>{% block title %}Welcome!{% endblock %}</title>
    
    {% block stylesheets %}
        {% stylesheets
        '@X4DSBAdmin2Bundle/Resources/public/css/bootstrap.min.css'
        '@X4DSBAdmin2Bundle/Resources/public/css/plugins/metisMenu/metisMenu.min.css'
        '@X4DSBAdmin2Bundle/Resources/public/css/sb-admin-2.css'
        '@X4DSBAdmin2Bundle/Resources/public/css/plugins/datagrid.css'
        'bundles/x4dsbadmin2/font-awesome/css/font-awesome.min.css'
        output='css/*.css' filter='cssrewrite' %}
        <link rel="stylesheet" type="text/css" media="screen" href="{{ asset_url }}" />
        {% endstylesheets %}

    {% endblock %}
        
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->

</head>

<body>

    <div class="container">
        {% block body %}{% endblock %}
    </div>
    
    {% block javascripts %}
        {% javascripts
        '@X4DSBAdmin2Bundle/Resources/public/js/jquery.js'
        '@X4DSBAdmin2Bundle/Resources/public/js/bootstrap.min.js'
        '@X4DSBAdmin2Bundle/Resources/public/js/plugins/metisMenu/metisMenu.min.js'
        '@X4DSBAdmin2Bundle/Resources/public/js/sb-admin-2.js'
        %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}
    {% endblock %}

</body>

</html>
